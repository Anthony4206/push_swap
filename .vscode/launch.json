    "version": "0.2.0",
    "configurations": [
        {
            "name": "C Launch",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "build_debug",
            "program": "${workspaceFolder}/push_swap_debug",
            "args": ["18", "20", "21"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "lldb",
            "environment": [
                { "name": "PATH", "value": "/usr/local/bin:/usar/bin:/bin:/usr/sbin:/sbin:/Library/Frameworks/Python.framework/Versions/3.10/bin:/Library/Frameworks/Python.framework/Versions/3.10/bin" }
            ],
            "showDisplayString": true
        }
    ]
}